generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")  // <-- Vérifie que c'est bien écrit comme ça
}

model Subscription {
  id        String   @id @default(cuid())
  name      String
  price     Float
  startDate DateTime
  category  String   @default("Autre") // Nouveau champ
  createdAt DateTime @default(now())
}